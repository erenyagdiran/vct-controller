<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
<Document>
    <name>community lab</name>
    <description><![CDATA[CONFINE Nodes recording info.]]></description>
    <Style id="markerStyle">
	    <IconStyle>
		    <Icon>
		        <href>https://dl.dropbox.com/u/36866466/mapicons/wifi.png</href>
	        </Icon>
	    </IconStyle>
    </Style>
  {% for location in locations %}
    <Placemark>
        <name>{{ location.name }}</name>
        <description><![CDATA[{{ location.description }}]]></description>
        <styleUrl>#markerStyle</styleUrl>
        <Point>
            <coordinates>{{location.lng}},{{location.lat}}</coordinates>
        </Point> 
    </Placemark>
  {% endfor %}
</Document>
</kml>
